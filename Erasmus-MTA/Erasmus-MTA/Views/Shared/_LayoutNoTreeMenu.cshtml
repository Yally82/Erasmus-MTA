<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="robots" content="index,follow" />

    <title>@ViewBag.Title</title>
    <link href="~/Content/themes/base/jquery.ui.all.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/jquery.jqGrid/ui.jqgrid.css" rel="stylesheet" type="text/css" />
    @*<script src="~/Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>*@             @*cu el nu merge bin toggle pentru navigation bar di stanga in care se afla jstree*@
    @*<script src="~/scripts/jquery-ui-1.10.4.min.js"></script>*@
    <script src="~/scripts/i18n/grid.locale-en.js" type="text/javascript"></script>*
    <script src="~/Scripts/jquery.jqGrid.min.js" type="text/javascript"></script>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jstree")

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>  @*este pentru pop-up....NU O MAI STERGEEEEEEEEE*@

    <script type="text/javascript" src="~/Scripts/jsgrid.min.js"></script>

</head>


<body>
    <div id="wrapperNoTree">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
            </div>




            <!-- Top Menu Items -->

            <ul class="nav collapse  navbar-collapse navbar-nav navbar-ex1-collapse">
                <li class="active">@Html.ActionLink("Home page", "Login", "Home")</li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Mobilitati incoming <span class="caret"></span></a>

                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Vizualizare mobilitati","Index", "Mobilities")</li>
                        <li>@Html.ActionLink("Participanti", "Index", "Person")</li>

                    </ul>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Mobilitati outcoming <span class="caret"></span></a>

                    <ul class="dropdown-menu">
                        <li>@Html.ActionLink("Vizualizare mobilitati", "Index", "Mobilities")</li>
                        <li>@Html.ActionLink("Participanti", "Index", "Person")</li>

                    </ul>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Vizualizare date <span class="caret"></span></a>

                    <ul class="dropdown-menu"> @*din linkurile de mai jos trebuie afisat intr-un header titlul din ActionLink-ul corespunzator*@
                        <li>@Html.ActionLink("Tari", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Orase", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Institutie", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Institutii partenere", "Index", "Tables")</li>

                        <li>@Html.ActionLink("Departamente", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Departamente partenere", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Personal ATM", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Participanti straini", "Index", "Tables")</li>
                        <li>@Html.ActionLink("Detalii mobilitati", "Index", "Tables")</li>
                    </ul>
            </ul>

        </nav>



        <script src="~/ScripturiRox/jstree.mobilities.js" type="text/javascript"></script>
        @RenderBody()
    </div>

</body>
</html>


